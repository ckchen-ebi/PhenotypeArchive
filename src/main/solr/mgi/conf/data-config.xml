<dataConfig>

	<dataSource name="mgi" type="HttpDataSource" baseUrl="http://www.knockoutmouse.org/" encoding="UTF-8"  connectionTimeout="5000" readTimeout="10000"/>

	<document name="docs">
		<entity dataSource="mgi" name="doc" stream="true" url="select?q=*:*&amp;fl=mgiid,chromosome,start,end&amp;rows=42292" processor="XPathEntityProcessor" forEach="/response/result/doc/" transformer="RegexTransformer" readTimeout="180000" connectionTimeout="60000">
			<field column="mgiid" xpath="/response/result/doc/str[@name='mgiid']" />
			<field column="chromosome" xpath="/response/result/doc/str[@name='chromosome']" />
			<field column="start" xpath="/response/result/doc/int[@name='start']" />
			<field column="end" xpath="/response/result/doc/int[@name='end']" />
		</entity>
	</document>

</dataConfig>
